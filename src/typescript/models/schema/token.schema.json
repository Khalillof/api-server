{
    "name":"token",
    "loadref":true,
    "schema": {
      "token": {
        "type": "String",
        "unique": true,
        "required": true,
        "minLength": 5,
        "maxLength": 250
     },
     "expiryDate": {
        "type": "Date",
        "required": true
    },
     "owner": {
        "type": "ObjectId",
        "ref": "account"
    }
     }
    }