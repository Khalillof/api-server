{
  "name": "category",
  "dependent": true,
  "useAuth": [
    "create",
    "update",
    "patch",
    "delete"
  ],
  "useAdmin": [
    "create",
    "update",
    "patch",
    "delete"
  ],
  "schemaObj": {
    "name": {
      "type": "String",
      "unique": true,
      "lowercase": true,
      "required": true,
      "minLength": 3,
      "maxLength": 30,
      "tag": "input",
      "className":"form-control"
    }
  }
}