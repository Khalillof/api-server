{
    "name": "post",
    "dependent": false,
    "pagesPerPage":5,
    "searchKey":"title",
    "displayName":"Blog",
    "queryName":"category",
    "useComment": true,
    "useLikes": true,
    "useAuth": [
        "create",
        "update",
        "patch",
        "delete"
    ],
    "useAdmin": [
        "create",
        "update",
        "patch",
        "delete"
    ],
    "plugins":[
        "comment","like"
       ],
    "schemaOptions": {},
    "schemaObj": {
        "title": {
            "type": "String",
            "required": true,
            "unique": true,
            "minLength": 5,
            "maxLength": 150,
            "tag": "input",
            "className":"form-control"
        },
      
        "descriptions": {
            "type": "String",
            "minLength": 5,
            "tag": "textarea",
            "className":"form-control",
            "style":{
                "minHeight": "150px"
              }
        },
        "body": {
            "type": "String",
            "required": true,
            "minLength": 5,
            "tag": "textarea",
            "html":"true",
            "className":"form-control",
            "style":{
                "minHeight": "200px"
              }
        },
        "isfeatured": {
            "type": "Boolean",
            "default": false,
            "tag": "input",
            "inputtype": "checkbox",
            "className":"form-check-input"
        },
        "isPublished": {
            "type": "Boolean",
            "default": false,
            "tag": "input",
            "inputtype": "checkbox",
            "className":"form-check-input"
        },
        "publisheDate": {
            "type": "Date",
            "tag": "input",
            "inputtype": "date",
            "className":"form-control"
        },
        "category": {
            "type": "ObjectId",
            "ref": "category",
            "required": true,
            "autopopulate": true,
            "tag": "select",
            "optionkey": "name",
            "className":"form-select"
        },
        "author": {
            "type": "ObjectId",
            "ref": "account",
            "required": true,
            "autopopulate": true
        },
        "image": {
            "type": "String",
            "minLength": 5,
            "maxLength": 150,
            "tag": "input",
            "className":"form-control"
        }
    },
    "template": ""
}