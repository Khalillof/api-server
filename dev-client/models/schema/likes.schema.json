{
    "name": "like",
    "active": true,
    "useAuth": [
        "post",
        "put",
        "delete"
    ],
    "useAdmin": ["search","count"],
    "schemaObj": {
        "like": {
            "type": "Boolean",
            "default": false,
            "tagname": "input",
            "className":"form-control"
        },
        "rate": {
            "type": "Number",
            "default": 0,
            "min": 0,
            "max": 5,
            "tagname": "input",
            "inputtype": "Number",
            "className":"form-control"
        },
        "comment": {
            "type": "String",
            "minLength": 0,
            "maxLength": 150,
            "tagname": "textarea",
            "className":"form-control"
        },
        "userid": {
            "type": "ObjectId",
            "ref": "account",
            "required": true,
            "autopopulate": true
        },
        "modelid": {
            "type": "ObjectId",
            "required": true
        }
    }
}