{ 
    "name":"like",
    "active":true,
    "useAuth":["post","put","delete"], 
    "useAdmin": [], 
      "schemaObj":{
        "like":  {
            "type":  "Boolean",
            "default":false
        },
        "rate":  {
            "type": "Number",
            "default": 0,
             "min": 0,
             "max": 5
        },
        "comment":  {
            "type": "String",
            "minLength": 0,
            "maxLength": 150
        },
        "author": {
            "type": "ObjectId",
            "ref": "account",
            "required":true,
            "autopopulate": true
        },
        "model_id": {
            "type": "ObjectId",
            "required":true
        }
    }
    }