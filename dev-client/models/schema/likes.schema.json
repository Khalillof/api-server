{
    "name": "like",
    "dependent": true,
    "useAuth": [
        "create",
        "update",
        "delete",
        "patch"
    ],
    
    "useAdmin": ["search"],
    "schemaObj": {
        "like": {
            "type": "Boolean",
            "default": false,
            "tag": "input",
            "className":"form-control"
        },
        "user": {
            "type": "ObjectId",
            "ref": "account",
            "required": true,
            "autopopulate": false
        },
        "modelid": {
            "type": "ObjectId",
            "required": true
        }
    }
}