{
  "name": "contact",
  "active": true,
  "searchKey": "fullname",
  "useAuth": [
    "list",
    "get",
    "post",
    "put",
    "delete",
    "search",
    "count"
  ],
  "useAdmin": [
    "list",
    "get",
    "put",
    "delete",
    "search",
    "count"
  ],
  "displayName":"Contacts",
  "schemaObj": {
    "contactType": {
      "type": "String",
      "required": true,
      "enum": [
        "consultation",
        "query",
        "hireme",
        "contact"
      ]
    },
    "fullname": {
      "type": "String",
      "required": true,
      "minLength": 3,
      "maxLength": 50,
      "tagname": "input",
      "className":"form-control"
    },
    "email": {
      "type": "String",
      "lowercase": true,
      "required": true,
      "minLength": 3,
      "maxLength": 30,
      "tagname": "input",
      "inputtype": "email",
      "className":"form-control"
    },
    "telphone": {
      "type": "string",
      "lowercase": true,
      "minLength": 3,
      "maxLength": 30,
      "tagname": "input",
      "inputtype": "telphone",
      "className":"form-control"
    },
    "message": {
      "type": "String",
      "required": true,
      "minLength": 5,
      "tagname": "textarea",
      "className":"form-control",
      "style":{
        "minHeight": "200px"
      }
    }
  }
}